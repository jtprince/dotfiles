#!/usr/bin/env python

import argparse
import subprocess

parser = argparse.ArgumentParser()
# in future get name from git!
parser.add_argument("name", help="the gitlab project name")
args = parser.parse_args()

PROJECT_IDS = dict(
    plotformer='13665454',
)

project_id = PROJECT_IDS[args.name]

cmd = str(
    f'http GET "https://gitlab.com/api/v4/projects/{project_id}/" '
    '"PRIVATE-TOKEN: $(cat $HOME/MEGA/env/cloud-and-apis/gitlab/access-token)" '
    '| jq -r .default_branch'
)

output = subprocess.check_output(cmd, shell=True, encoding='utf-8')
print(output)
